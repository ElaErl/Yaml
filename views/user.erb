<h4> Name: </h4> 
<p> <%= @user_name.capitalize %> </p>
<h4> E-mail: </h4> 
<p> <%= @email %> </p>
<h4> Interests: </h4>
<p><%= @interests.join(", ") %></p>

<h5>Other users:</h5>

<ul>
<% @users.keys.each do |user| %>
  <% next if user == @user_name %>
  <li>
    <a href="<%= user %>"><%= user %></a>
  </li>
<% end %>
</ul>
